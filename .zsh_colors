## .zsh_colors file for managing terminal colors
## created 8/12/2015 by Kuen

# Clear LSCOLORS
unset LSCOLORS
# Redefine terminal colors for black background
export CLICOLOR=1
export LSCOLORS=gxfxcxdxbxegedabagacad
#export ZLS_COLORS=$LSCOLORS

# Color differentiation for different file extensions
# Attribute codes:
# 00=none 01=bold 04=underscore 05=blink 07=reverse 08=concealed
# Text color codes:
# 30=blk 31=red 32=grn 33=yel 34=blu 35=mag 36=cyn 37=wht
# Background color codes:
# 40=blk 41=red 42=grn 43=yel 44=blu 45=mag 46=cyn 47=wht

ZLS_COLORS='rs=0:di=01;34:ln=01;36:mh=00:pi=40;33:so=01;35:do=01;35:
:bd=40;33;01:cd=40;33;01:or=40;31;01:su=37;41:sg=30;43:ca=30;41:
:tw=30;42:ow=34;42:st=37;44:ex=01;32:

:*.zzz=00:*.avi=01;36:*.m4v=01;36:*.mkv=01;36:*.mp4=01;36:*.wmv=01;36:
:*.ogm=01;36:*.m2v=01;36:*.asf=01;36:*.rm=01;36:*.rmvb=01;36:
:*.flc=00;36:*.fli=00;36:*.flv=00;36:*.qt=00;36:*.mov=00;36:
:*.mpg=01;33:*.mpeg=01;33:*.ts=00;33:*.vob=01;33:

:*.zzz=00:*.jpg=01;32:*.JPG=01;32:*.jpeg=01;36:*.gif=01;36:
:*.bmp=01;33:*.tiff=01;33:*.tif=01;33:*.dng=01;33:*.DNG=01;33:
:*.png=01;33:*.svg=00;36:*.svgz=00;36:*.mng=00;36:*.pcx=00;36:

:*.zzz=00::*.aac=00;36:*.mp3=00;36:*.mka=00;36:*.m4a=00;36:*.mpc=00;36:
:*.flac=01;36:*.ogg=01;36:*.oga=01;36:*.wav=01;36:
:*.mid=00;36:*.midi=00;36:*.ra=00;36:';

export ZLS_COLORS
